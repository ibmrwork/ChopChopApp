@model ChopChop.ViewModel.ViewModel.VendorViewModel
@{
    ViewBag.Title = "AddVendor";
}
@{
    Layout = null;
}
@Styles.Render("~/Content/c1")
<link href="~/Content/styles/style.css" rel="stylesheet" />
<link href="~/Content/ApplicationCss/image-button.css" rel="stylesheet" />
@using (Html.BeginForm("SaveVendor", "Vendor", new { area = "Vendor" }, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

    <div class="wrapper">
        <header>
            <a href="#" title="CHOP CHOP" class="logo"><img src="~/Content/images/logo.png" alt="CHOP CHOP"></a>
        </header>
        <div class="jumbotron" style="margin-top:50px;height:500px;">
            <nav class="navbar-inverse" style="margin-top:-48px;height:60px;">
                <div class="col-sm-3 col-md-3 pull-left">
                    <h4 class="bottom" style="color:red;font-size:15px;">Restaurant Management</h4>
                </div>
            </nav>
            <div class="container" style="margin-top:20px;">
                <div class="form-horizontal">
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    <div class="row">
                        <div class="col-lg-1"></div>
                        <div class="col-lg-4">
                            <div class="form-group">
                                <div class="col-md-2">
                                    <label class="control-label col-sm-1">Name</label>
                                </div>
                                <div class="col-md-10">
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(model => model.VendorName, new { @class = "form-control", @id = "txtVendorName" })
                                        @Html.ValidationMessageFor(model => model.VendorName, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-2">
                                    <label class="control-label col-sm-1">Styles</label>
                                </div>
                                <div class="col-md-10">
                                    <div class="col-md-10">
                                        @*@Html.TextBoxFor(model => model.VendorStyle, new { @class = "form-control" })*@
                                        @Html.DropDownListFor(model => model.VendorStyle, new SelectList(ViewBag.Styles, "TypeID", "Name"),
                        "Choose Style",
                        new { @class = "form-control", @id = "ddlStyles" })
                                        @Html.ValidationMessageFor(model => model.VendorStyle, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-2">
                                    <label class="control-label col-md-1">Phone</label>
                                </div>
                                <div class="col-md-10">
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(model => model.Phone, new { @class = "form-control", @id = "txtPhone" })
                                        @Html.ValidationMessageFor(model => model.Phone, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-2">
                                    <label class="control-label col-md-1">Address</label>
                                </div>
                                <div class="col-md-10">
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(model => model.AddressLine1, new { @class = "form-control", @id = "txtAddress" })
                                        @Html.ValidationMessageFor(model => model.AddressLine1, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            Opening Hours:
                            <div class="form-group">
                                <div class="col-md-2">
                                    <label class="control-label col-md-1">Lunch</label>
                                </div>
                                <div class="col-md-5">
                                    <div class="col-md-10">
                                        <select id="ddlLunchTimeWeakDay" name="LunchTimeWeakDay" class="form-control">
                                            <option value="0" selected>Select Time </option>
                                            <option value="10AMTo3PM">10 AMTo 3PM</option>
                                        </select>

                                    </div>
                                </div>
                                <div class="col-md-5">
                                    <div class="col-md-10">
                                        @Html.DropDownListFor(model => model.LunchWeakDays, ViewBag.EnumListWeakDays as SelectList, "Day", new { @class = "form-control", @id = "ddlLunchWeakDay" })

                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-2">

                                </div>
                                <div class="col-md-5">
                                    <div class="col-md-10">
                                        <select class="form-control" name="LunchTimeWeakEnd" id="ddlLunchTimeWeakEnd">
                                            <option value="0" selected>Select Time </option>
                                            <option value="9AMTo4PM">9AM To 4PM</option>
                                        </select>

                                    </div>
                                </div>
                                <div class="col-md-5">
                                    <div class="col-md-10">
                                        @Html.DropDownListFor(model => model.LunchWeakEnd, ViewBag.EnumListWeakEnd as SelectList, "Day", new { @class = "form-control", @id = "ddlLunchWeakEnd" })

                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-2">
                                    <label class="control-label col-md-1">Dinner</label>
                                </div>
                                <div class="col-md-5">
                                    <div class="col-md-10">
                                        <select class="form-control" name="DinnerTimeWeakDay" id="ddlDinnerTimeWeakDay">
                                            <option value="0" selected>Select Time </option>
                                            <option value="5PMTo10PM">5PM To 10PM</option>
                                        </select>

                                    </div>
                                </div>
                                <div class="col-md-5">
                                    <div class="col-md-10">
                                        @Html.DropDownListFor(model => model.DinnerWeakDays, ViewBag.EnumListWeakDays as SelectList, "Day", new { @class = "form-control", @id = "ddlDinnerWeakDay" })

                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-2">

                                </div>
                                <div class="col-md-5">
                                    <div class="col-md-10">
                                        <select class="form-control" name="DinnerTimeWeakEnd" id="ddlDinnerTimeWeakEnd">
                                            <option value="0" selected>Select Time </option>
                                            <option value="5PMTo11PM">5PM To 11PM</option>
                                        </select>

                                    </div>
                                </div>
                                <div class="col-md-5">
                                    <div class="col-md-10">
                                        @Html.DropDownListFor(model => model.DinnerWeakEnd, ViewBag.EnumListWeakEnd as SelectList, "Day", new { @class = "form-control", @id = "ddlDinnerWeakEnd" })

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">

                            <div class="form-group">
                                <div class="col-md-4">
                                    <img id="Main" src="data:image/png;base64," style="width:200px;height:150px" />
                                    <div class="row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-6">
                                            <label class="btn btn-primary btn-file">
                                                Browse <input type="file" name="pictureMain" id="imgMain" style="display: none;">
                                            </label>
                                        </div>
                                        <div class="col-lg-5"></div>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <img id="Other" src="data:image/png;base64," style="width:200px;height:150px" />
                                    <div class="row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-6">
                                            <label class="btn btn-primary btn-file">
                                                Browse <input type="file"  name="PictureOther" id="imgOther" style="display: none;">
                                            </label>
                                        </div>
                                        <div class="col-lg-5"></div>
                                    </div>
                                </div>

                                <div class="col-md-4">
                                    <img id="Logo" src="data:image/png;base64," style="width:200px;height:150px" />
                                    <div class="row">
                                        <div class="col-lg-1"></div>
                                        <div class="col-lg-6">
                                            <label class="btn btn-primary btn-file">
                                                Browse <input type="file" name="PictureLogo" id="imgLogo" style="display: none;">
                                            </label>
                                        </div>
                                        <div class="col-lg-5"></div>
                                    </div>
                                </div>

                            </div>
                            <div class="form-group">
                                <div class="col-md-1">
                                    <label class="control-label col-md-1">360</label>
                                </div>
                                <div class="col-md-11">
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(model => model.C360DegreePath, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.C360DegreePath, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-1">
                                    <label class="control-label col-md-1">Video</label>
                                </div>
                                <div class="col-md-3">

                                    <div class="col-md-10">
                                        <button class="btn btn-primary" type="button" style="width:140px;">Upload Video</button>
                                    </div>

                                </div>
                                <div class="col-md-1">
                                    <label class="control-label col-md-1">Or</label>
                                </div>
                                <div class="col-md-7">
                                    <div class="form-group">
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(model => model.MediaPath, new { @class = "form-control" })
                                            @Html.ValidationMessageFor(model => model.MediaPath, "", new { @class = "text-danger" })
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="form-group">
                                <div class="col-md-1">
                                    <label class="control-label col-md-1">Blurb</label>
                                </div>
                                <div class="col-md-11">
                                    <div class="col-md-10">
                                        @Html.TextAreaFor(model => model.Blurb, new { @class = "form-control" })
                                        @Html.ValidationMessageFor(model => model.Blurb, "", new { @class = "text-danger" })
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-offset-2 col-md-10">
                                    <button id="btnSaveVendor" type="submit" class="btn btn-primary">Done</button>

                                </div>
                            </div>

                        </div>
                        <div class="col-lg-1">

                        </div>

                    </div>
                </div>

            </div>
        </div>
    </div>


}



@Scripts.Render("~/bundles/b1")
<script src="~/Scripts/Vendor/image-upload.js"></script>
@*<script src="~/Scripts/Vendor/add-vendor.js"></script>*@


